{
    "name": "weaviate-outline",
    "displayName": "Weaviate Code Outline",
    "description": "Provides code outline using Weaviate-backed symbol tracking",
    "version": "0.1.0",
    "publisher": "larrak",
    "engines": {
        "vscode": "^1.80.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "activationEvents": [
        "onLanguage:python"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "configuration": {
            "title": "Weaviate Outline",
            "properties": {
                "weaviateOutline.apiUrl": {
                    "type": "string",
                    "default": "http://localhost:5001",
                    "description": "URL of the Weaviate outline API server"
                },
                "weaviateOutline.enabled": {
                    "type": "boolean",
                    "default": true,
                    "description": "Enable Weaviate-backed outline"
                }
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./"
    },
    "devDependencies": {
        "@types/node": "^18.0.0",
        "@types/vscode": "^1.80.0",
        "typescript": "^5.0.0"
    },
    "dependencies": {
        "axios": "^1.6.0"
    }
}